{
  "name": "nuxt-vuetify-template",
  "version": "1.0.0",
  "description": "Template Nuxtjs + Vuetify",
  "author": "Ainun Abdullah",
  "private": true,
  "scripts": {
      "dev": "nuxt",
      "build": "nuxt build",
      "start": "nuxt start",
      "build:dev": "cross-env NODE_ENV=development nuxt build",
      "start:dev": "cross-env NODE_ENV=development nuxt start",
      "generate": "nuxt generate",
      "lint": "eslint --ext .js,.vue --ignore-path .gitignore --ignore-path .eslintignore .",
      "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore --ignore-path .eslintignore .",
      "docs:gen": "vuese gen && vue-styleguidist build --config styleguide.config.js && jsdoc -c jsdoc.conf",
      "docs:serve": "serve docs"
  },
  "dependencies": {
      "@nuxtjs/axios": "5.9.5",
      "@nuxtjs/sitemap": "^2.0.1",
      "cookie-universal-nuxt": "2.1.2",
      "cookieparser": "0.1.0",
      "highcharts": "8.1.1",
      "highcharts-vue": "1.3.5",
      "js-cookie": "2.2.1",
      "nuxt": "2.11.0",
      "object-to-formdata": "2.1.2",
      "vue-filter-date-format": "1.3.1",
      "vue-filter-date-parse": "1.1.3",
      "vue-quill-editor": "3.0.6",
      "vue-upload-component": "2.8.20",
      "vue2-filters": "0.9.1",
      "vue2-perfect-scrollbar": "^1.3.0",
      "vuetify": "2.2.15"
  },
  "devDependencies": {
      "@nuxtjs/dotenv": "1.4.1",
      "@nuxtjs/eslint-config": "2.0.2",
      "@nuxtjs/eslint-module": "1.1.0",
      "@nuxtjs/google-analytics": "^2.2.3",
      "@nuxtjs/router": "1.5.0",
      "@nuxtjs/vuetify": "1.11.0",
      "@vuese/cli": "2.8.3",
      "babel-eslint": "10.1.0",
      "cross-env": "7.0.0",
      "eslint": "6.8.0",
      "eslint-loader": "3.0.3",
      "eslint-plugin-nuxt": ">=0.4.2",
      "eslint-plugin-vue": "6.2.1",
      "serve": "11.3.0",
      "vue-styleguidist": "4.13.1"
  },
  "precommit": "yarn lint"
}
